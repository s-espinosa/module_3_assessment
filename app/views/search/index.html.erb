<h1>Stores</h1>

<p><%= @stores.total %> Total Stores</p>

<table>
  <tr>
    <th>Number</th>
    <th>Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone</th>
    <th>Type</th>
  </tr>
    <% @stores.each do %>
      <tr>
        <td>@store.number</td>
        <td>@store.name</td>
        <td>@store.city</td>
        <td>@store.distance</td>
        <td>@store.phone</td>
        <td>@store.type</td>
      </tr>
    <% end %>
</table>
